{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ContentModule": {
    "SiteTitle": "NantCom",
    "StandardPages": [
      {
        "Url": "/",
        "View": "Home",
        "Title": "Home"
      },
      {
        "Url": "/portfolio",
        "View": "Portfolio-List",
        "Title": "Portfolio"
      },
      {
        "Url": "/portfolio/portfolio1",
        "View": "Portfolio-Item",
        "Title": "Portfolio Item 1"
      },
      {
        "Url": "/blog",
        "View": "Blog-List",
        "Title": "Blog"
      },
      {
        "Url": "/blog/blog1",
        "View": "Blog-Item",
        "Title": "Blog Item 1"
      }
    ],
    "PageModels": {
      "/": "NC.WebEngine.ViewModels.HomeModel"
    }
  },

  "AllowedHosts": "*"
}
