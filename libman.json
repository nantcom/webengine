      "files": [
        "dist/bundle.js",
        "dist/bundle.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@editorjs/embed@2.5.1",
      "destination": "wwwroot/js/editorjs/embed/",
      "files": [
        "dist/bundle.js",
        "dist/bundle.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "editorjs-alert@latest",
